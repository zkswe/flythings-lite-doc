{
    "title": "FlyThings Lite Docs",
    "language": "zh-hans",
    "links": {
        "sidebar": {
            "首页": "/"
        }
     },
    "plugins": [
      "-lunr", "-search", "-sharing", "search-pro",
      "splitter",
      "chapter-fold",
      "expandable-chapters",
      "ga",
      "back-to-top-button",
      "anchor-navigation-ex",
      "flexible-alerts",
      "custom-favicon",
      "scripts",
      "tbfed-pagefooter",
      "book-summary-scroll-position-saver"
    ],

    "pluginsConfig": {
      "tbfed-pagefooter": {
        "copyright":"",
        "modify_label": "last modified:",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
      "ga": {
        "token": "UA-121188128-3"
      },
      "favicon": "favicon.ico",
      "anchor-navigation-ex": {
            "showLevel": false,
            "showGoTop": false
      },
      "scripts": {
            "files": [
                "vendor.js"
            ]
      }
    }
}
